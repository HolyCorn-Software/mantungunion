{
    "name": "web",
    "label": "Web",
    "init": "./exec.mjs",
    "http": {
        "staticPaths": {
            "/$/shared/static/": "./shared/",
            "/$/web/html/": "./html",
            "/": "./html"
        }
    },
    "meta": {
        "settings": {
            "widgets": {
                "public": true,
                "label": "Widgets public settings"
            }
        },
        "modernuser": {
            "permissions": [
                {
                    "name": "permissions.mantungunion.web.gallery.manage",
                    "label": "Alter information about the events gallery"
                }
            ]
        }
    }
}